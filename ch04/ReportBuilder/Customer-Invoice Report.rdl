  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <CommandText>SELECT
  Customer.BillingCity
  ,Customer.Name
  ,InvoiceHeader.InvoiceNumber
  ,InvoiceHeader.InvoiceDate
  ,InvoiceHeader.TotalAmount
FROM
  Customer
  LEFT OUTER JOIN InvoiceHeader
    ON Customer.CustomerNumber = InvoiceHeader.CustomerNumber</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <SelectedColumns>
