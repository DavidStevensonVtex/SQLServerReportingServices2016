              <ColumnExpression ColumnOwner="InvoiceHeader" ColumnName="InvoiceDate" />
              <ColumnExpression ColumnOwner="InvoiceHeader" ColumnName="TotalAmount" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Customer" />
              <Join Type="LEFT OUTER JOIN" ColumnOwner="InvoiceHeader">
                <On>
                  <From ColumnOwner="Customer" ColumnName="CustomerNumber" />
                  <To ColumnOwner="InvoiceHeader" ColumnName="CustomerNumber" />
                </On>
              </Join>
            </Joins>
          </QueryDefinition>
        </rd:DesignerState>
